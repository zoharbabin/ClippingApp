<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.HScrollBar")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="inactive" />
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>
	
	<!--- Defines the skin class for the HScrollBarSkin's track. -->
	<s:Button id="track" top="0" right="2" bottom="2" left="2" height="12" 
			  skinClass="skins.list.ListHScrollerTrackBtnSkin" />
	
	<!--- Defines the skin class for the HScrollBarSkin's thumb. The default skin class is HScrollBarThumbSkin. -->
	<s:Button id="thumb" focusEnabled="false" visible.inactive="false" height="5" top="4" 
			  skinClass="skins.list.ListHScrollerThumbBtnSkin"/>
	<!-- SkinParts
	name=decrementButton, type=spark.components.Button, required=false
	name=incrementButton, type=spark.components.Button, required=false
	name=track, type=spark.components.Button, required=false
	name=thumb, type=spark.components.Button, required=false
	-->
</s:Skin>
