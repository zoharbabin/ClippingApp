<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="disabled" />
		<s:State name="down" />
		<s:State name="over" />
		<s:State name="up" />
	</s:states>
	
	<!-- background -->
	<s:Rect width="11" height="11" >
		<s:fill>
			<s:SolidColor color="0x0" alpha="0" />
		</s:fill>
	</s:Rect>
	
	<s:BitmapImage source="@Embed('/assets/list/item/delete_button_icon_up.png')" 
				   left="2" top="2"
				   width="7" height="7" smooth="true" includeIn="up" />
	<s:BitmapImage source="@Embed('/assets/list/item/delete_button_icon_up.png')" 
				   left="2" top="2"
				   width="7" height="7" smooth="true" includeIn="over" />
	<s:BitmapImage source="@Embed('/assets/list/item/delete_button_icon_over.png')" 
				   left="2" top="2"
				   width="7" height="7" smooth="true" includeIn="disabled" />
	<s:BitmapImage source="@Embed('/assets/list/item/delete_button_icon_over.png')" 
				   left="2" top="2"
				   width="7" height="7" smooth="true" includeIn="down" />
	
	<!-- SkinParts
	name=labelDisplay, type=spark.core.IDisplayText, required=false
	name=iconDisplay, type=spark.primitives.BitmapImage, required=false
	-->
</s:Skin>
